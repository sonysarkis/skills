{
    "name": "sonysarkis/skills",
    "description": "Laravel package to interact with DummyJson",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "sonysarkis",
            "email": "sonygomez6@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "saloonphp/saloon": "v3.x-dev"
    },
    "autoload": {
        "psr-4": {
            "Sonysarkis\\Skills\\": "src/",
            "Sonysarkis\\Skills\\Tests\\": "tests/",
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sonysarkis\\Skills\\SkillsServiceProvider"
            ]
        }
    },
    "require-dev": {
        "pestphp/pest": "3.x-dev"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
